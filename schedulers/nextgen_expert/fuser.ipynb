    {
      "cell_type": "code",
      "metadata": { "language": "python" },
      "source": [
        "# Load GCS summary rows (written by gcs_scheduler.write_summary)",
        "summary_path = OUTDIR / \"summary.csv\"",
        "summary = pd.read_csv(summary_path) if summary_path.exists() else pd.DataFrame()",
